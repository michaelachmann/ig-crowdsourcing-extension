{
  "name": "IG-Story Crowdsourcing Plattform",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Extension for coding and transcribing Instagram Stories for Social Media Research",
  "homepage_url": "https://ig-lab.net",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    "inject/inject.html",
    "app/index.html",
    "app/script.js"
  ],
  "permissions": [
    "https://*.instagram.com/*",
    "https://instagram.com/*",
    "webRequest",
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": [
        "inject/appwrite.js",
        "inject/jquery-3.6.0.min.js",
        "inject/inject.js"
      ]
    }
  ]
}