{
  "name": "Instagram Story Annotator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "bg/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": [
    "inject/inject.html",
    "app/index.html",
    "app/script.js"
  ],
  "options_page": "options/index.html",
  "permissions": [
    "https://*.instagram.com/*",
    "https://instagram.com/*",
    "webRequest",
    "tabs",
    "debugger"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": [
        "inject/jquery-3.6.0.min.js",
        "inject/inject.js"
      ]
    }
  ]
}